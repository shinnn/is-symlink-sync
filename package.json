{
  "name": "is-symlink-sync",
  "version": "1.0.0",
  "description": "Synchronously check if a file is a symbolic link",
  "repository": "shinnn/is-symlink-sync",
  "scripts": {
    "pretest": "jscs *.js && eslint *.js",
    "test": "node test.js | tap-spec",
    "coverage": "istanbul cover test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/is-symlink-sync/blob/master/LICENSE"
    }
  ],
  "files": [
    "index.js"
  ],
  "keywords": [
    "file",
    "lstat",
    "stat",
    "stats",
    "sync",
    "synchronous",
    "symbolic link",
    "link",
    "symlink",
    "check",
    "detect"
  ],
  "dependencies": {
    "graceful-fs": "^3.0.5",
    "tryit": "^1.0.1"
  },
  "devDependencies": {
    "eslint": "^0.14.1",
    "istanbul": "^0.3.5",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.11.3",
    "tap-spec": "^2.2.1",
    "tape": "^3.5.0"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98,
    "requireBlocksOnNewline": true,
    "validateLineBreaks": "LF"
  }
}
